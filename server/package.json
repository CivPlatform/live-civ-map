{
	"name": "live-civ-map-server",
	"version": "0.0.0",
	"private": true,
	"author": "Gjum",
	"license": "All Rights Reserved",
	"scripts": {
		"pg:init": "initdb postgres/ && echo \"unix_socket_directories = './'\" >> postgres/postgresql.conf",
		"pg:start": "pg_ctl start -D postgres/",
		"pg:stop": "pg_ctl stop -D postgres/",
		"psql": "psql -h localhost postgres",
		"start": "node dist/main.js",
		"start:dev": "yarn build && node -r dotenv/config dist/main.js",
		"build": "tsc"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 2,
		"trailingComma": "es5",
		"singleQuote": true,
		"semi": false
	},
	"dependencies": {
		"@types/jsonwebtoken": "^8.5.8",
		"jsonwebtoken": "^8.5.1",
		"node-fetch": "2",
		"node-static": "^0.7.11",
		"pg": "^8.7.1",
		"uuid": "^8.3.2",
		"ws": "^8.3.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.10",
		"@types/node-fetch": "^2.6.1",
		"@types/node-static": "^0.7.7",
		"@types/pg": "^8.6.1",
		"@types/uuid": "^8.3.3",
		"@types/ws": "^8.2.0",
		"dotenv": "^10.0.0",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2"
	}
}
